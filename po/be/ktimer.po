#: prefwidget.ui:59
#, kde-format
msgid "Add a new task"
msgstr "Дадаць новую задачу"

#. i18n: ectx: property (whatsThis), widget (QPushButton, m_add)
#: prefwidget.ui:62
#, kde-format
msgid "Add a new task to the list."
msgstr "Дадаць новую задачу ў спіс."

#. i18n: ectx: property (text), widget (QPushButton, m_add)
#: prefwidget.ui:65
#, kde-format
msgid "&New"
msgstr "&Стварыць"

