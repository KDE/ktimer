#: prefwidget.ui:148
#, kde-format
msgid "||"
msgstr "||"

#. i18n: ectx: property (toolTip), widget (QToolButton, m_start)
#: prefwidget.ui:155
#, kde-format
msgid "Start a countdown"
msgstr "Démarrer un compte à rebours"

