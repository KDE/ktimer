#: prefwidget.ui:222
#, kde-format
msgid "Delay:"
msgstr "Atraso:"

#. i18n: ectx: property (toolTip), widget (QSpinBox, m_delay)
#: prefwidget.ui:229
#, kde-format
msgid "Enter the seconds here"
msgstr "Indique aqui os segundos"

