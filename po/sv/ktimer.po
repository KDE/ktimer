#: prefwidget.ui:65
#, kde-format
msgid "&New"
msgstr "&Ny"

#. i18n: ectx: property (toolTip), widget (QPushButton, m_remove)
#: prefwidget.ui:72
#, kde-format
msgid "Remove a task"
